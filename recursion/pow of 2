import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

class backtrack{
    public static boolean solve(int n , int num) {
        long sum = (long)Math.pow(2,num );
        if(n==sum){
            return true ;
        }
        if(n<sum){
            return false;
        }

        return solve(n, num+1);
    }
    
    public static void main(String[] args) {
     int n = 16 ;
     System.out.println(solve(n , 0));
    
       
    
    }
}
