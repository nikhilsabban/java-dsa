https://leetcode.com/problems/fibonacci-number/
class Solution {
     public static int solve(int num , int f0 , int f1 ){
        if(num ==1){
            return f1 ;
        }
        if(num==0){
            return 0;
        }
        
        return solve(num-1,f1 , f0+f1);
      
    }
    public int fib(int n) {
        
         return solve(n , 0 ,1 );
    }
}
